;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="O Misterio das Cartas"
run/main_scene="res://gui/landing.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://assets/spalshScreen.png"
boot_splash/fullsize=false
config/icon="res://icon512.png"

[aseprite_importers]

aseprite_executable_path="C:/Program Files (x86)/Steam/steamapps/common/Aseprite/Aseprite.exe"

[autoload]

Globals="*res://globals.gd"
Helper="*res://utils/helper.gd"
InteractionCall="*res://interactable/InteractionUI.tscn"
Enums="*res://utils/enums.gd"
Shaker="*res://addons/shaker/src/Shaker.gd"
Hud="*res://gui/HUD.tscn"

[display]

window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/shaker/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red",
"res://cards/": "orange",
"res://conversation/": "gray",
"res://enemies/": "blue",
"res://gui/": "yellow",
"res://interactable/": "gray",
"res://inventory/": "gray",
"res://levels/": "green",
"res://player/": "purple",
"res://props/": "pink",
"res://utils/": "gray"
}

